ARCHENGINE_VERSION_MAJOR=2
ARCHENGINE_VERSION_MINOR=7
ARCHENGINE_VERSION_PATCH=0
ARCHENGINE_VERSION="$ARCHENGINE_VERSION_MAJOR.$ARCHENGINE_VERSION_MINOR.$ARCHENGINE_VERSION_PATCH"

ARCHENGINE_RELEASE_DATE=`date "+%B %e, %Y"`

ARCHENGINE_VERSION_STRING="ArchEngine $ARCHENGINE_VERSION: ($ARCHENGINE_RELEASE_DATE)"

ARCHENGINE_VERSION_UNIQUE_NAME=`printf "_%d%03d" $ARCHENGINE_VERSION_MAJOR $ARCHENGINE_VERSION_MINOR`
